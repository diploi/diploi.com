{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "src",
    "src/**/*.d.ts"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "tsx": "react-tsx",
    "tsxImportSource": "react",
    "jsxImportSource": "react",
    "allowJs": true,
    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ]
  }
}